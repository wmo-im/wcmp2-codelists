@prefix skos:  <http://www.w3.org/2004/02/skos/core#> . 
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<host> a skos:Concept ;
	rdfs:label "host" ;
	skos:notation "host" ;
	dct:description "The host is the actual provider offering the data on their storage. There should be no more than one host, specified as last element of the list";
	owl:sameAs "{source}"@en	.
